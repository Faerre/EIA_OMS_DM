CREATE OR REPLACE VIEW DWSEAI01.OMS_MKTG_TEMP_V AS SELECT YEARMONTH,
  BUS_LINE,
  ITEM_CATEGORY,
  ITEM_SUBCATEGORY,
  P_GROUP,
  FAMILY,
  BRAND,
  SUB_BRAND,
  SKU,
  BUS_LINE_EUR,
  BRAND_EUR,
  ITEM_CATEGORY_EUR,
  ITEM_SUBCATEGORY_EUR,
  FAMILY_EUR,
  PRODUCT_NAME_EUR,
  PRODUCT_NAME_GLBL,
  IMC_KEY_NO,
  CAST(NEW_APP_FLAG AS INTEGER) AS NEW_APP_FLAG,
  TENURE,
  U35_FLAG,
  COUNTRY,
  COUNTRIES.SUBREGION,
  COUNTRIES.CLUSTER_NAME,
  GL_SEGMENT,
  SALES_REGION,
  STATE,
  IMC_TYPE,
  DIST_TYPE,
  BV_USD_TTL,
  PV_TTL,
  OMS_UNIT_CNT_TTL,
  BUYER_FLAG_TTL,
  BUYERS_CNT_TTL,
  R_IMC_TTL,
  PV,
  BV_USD,
  OMS_UNIT_CNT,
  BL_BUYER_FLAG,
  CAT_BUYER_FLAG,
  SUBCAT_BUYER_FLAG,
  BR_BUYER_FLAG,
  S_BR_BUYER_FLAG,
  P_GROUP_BUYER_FLAG,
  FAMILY_BUYER_FLAG,
  SKU_BUYER_FLAG,
  SKU_BUYERS_CNT,
  BL_PV_EUR,
  BR_PV_EUR,
  CAT_PV_EUR,
  SUBCAT_PV_EUR,
  FAMILY_PV_EUR,
  TTL_PV_EUR,
  BL_BUYERS_CNT_EUR,
  BR_BUYERS_CNT_EUR,
  CAT_BUYERS_CNT_EUR,
  SUBCAT_BUYERS_CNT_EUR,
  FAMILY_BUYERS_CNT_EUR,
  R_IMC_BL,
  R_IMC_CAT,
  R_IMC_SUBCAT,
  R_IMC_P_GROUP,
  R_IMC_FAMILY,
  R_IMC_BR,
  R_IMC_S_BR,
  USD
FROM OMS_MKTG_TEMP T0
LEFT JOIN COUNTRIES COUNTRIES
ON T0.COUNTRY=COUNTRIES.CNTRY_SHORT_NM
